#, c-format
msgid " install      - Install/upgrade packages\n"
msgstr " install      - Instala/atualiza pacotes\n"

#: src/main.cc:154
#, c-format
msgid " remove       - Remove packages\n"
msgstr " remove       - Remove pacotes\n"

